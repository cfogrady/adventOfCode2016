<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.0.xsd"
>

	<bean id="solution" class="com.galatea.chris.advent2016.solutions.Day8Solution">
	    <constructor-arg ref="fileLineFetcher"></constructor-arg>
	</bean>
	
	<bean id="fileLineFetcher" class="com.galatea.chris.advent2016.fetchers.FileLineFetcher">
	    <constructor-arg value="Day8/input.txt" type="String"></constructor-arg>
	</bean>
	
</beans>